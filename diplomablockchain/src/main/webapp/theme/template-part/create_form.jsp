<form method="post" action="" class="feedback__form">
    <div class="feedback__row">
            <div class="feedback__input">
                <label for="">
                    First name
                </label>
                <input required type="text" name="name" placeholder="Andrew">
            </div>
            <div class="feedback__input">
                <label for="">
                    Surname
                </label>
                <input required type="text" name="surname"
                       placeholder="Smith">
            </div>
            <div class="feedback__input">
                <label for="">
                    Mark
                </label>
                <input required type="tel" name="phone"
                       placeholder="5">
            </div>
            <div class="feedback__input">
                <label for="">
                    Mark letter
                </label>
                <input required type="text" name="sex"
                       placeholder="A">
            </div>
           
    </div>
    <div class="form__bottom">
            <button type="submit" class="feedback__btn orange-btn">
                New Blok
            </button>
    </div>
</form>
